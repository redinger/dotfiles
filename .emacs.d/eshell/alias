alias rtags ctags-exuberant -e --Ruby-kinds=-fFcm -o TAGS -R $*
alias s ssh $*
alias r rgrep $*
alias l ls $*
alias g git $*
alias f find $*
alias c curl $*
alias emacs find-file $1
alias lh ls -l -h $*
alias ll ls -l $*
alias la ls -a $*
alias gst git status
alias gb git branch $*
alias gbv git branch -v $*
alias gco git checkout $*
alias glt git log -n 10
alias gsu git submodule update --init --recursive
alias be bundle exec $*
alias gdum git commit --allow-empty -m dummy
